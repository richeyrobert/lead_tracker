<%= form_for(@agent) do |f| %>
  <% if @agent.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@agent.errors.count, "error") %> prohibited this agent from being saved:</h2>

      <ul>
      <% @agent.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :agent_type %><br />
    <%#= f.text_field :agent_type %>
    <%= f.collection_select(:agent_type_id, AgentType.all, :id, :agent_type, :prompt => "Select an Agent Type") %>
  </div>
  <div class="field">
    <%= f.label :agent_status %><br />
    <%#= f.text_field :agent_status %>
    <%= f.collection_select(:agent_status_id, AgentStatus.all, :id, :status, :prompt => "Select an Agent Status") %>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :phone1 %><br />
    <%= f.text_field :phone1 %>
  </div>
  <div class="field">
    <%= f.label :phone2 %><br />
    <%= f.text_field :phone2 %>
  </div>
  <div class="field">
    <%= f.label :address1 %><br />
    <%= f.text_field :address1 %>
  </div>
  <div class="field">
    <%= f.label :address2 %><br />
    <%= f.text_field :address2 %>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </div>
  <div class="field">
    <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </div>
  <div class="field">
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </div>
  <div class="field">
    <%= f.label :partner %><br />
    <%#= f.text_field :partner %>
    <%= f.collection_select(:partner_id, Partner.all, :id, :name, :prompt => "Select a Partner") %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
